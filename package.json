{
  "name": "@neo9/n9-node-conf",
  "version": "1.0.6",
  "description": "Neo9 Node Conf Module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "scripts": {
    "build": "rimraf dist/ && tsc",
    "lint": "tslint src/*.ts",
    "test": "npm run lint && npm run build && nyc --reporter=lcov ava --verbose --serial dist/test/ && n9-sonar-generate",
    "preversion": "npm test",
    "prepublish": "npm run build"
  },
  "sonar": {
    "coverage.exclusions": "src/*.d.ts",
    "ts.coverage.lcovReportPath": "./coverage/lcov.info",
    "ts.tslint.path": "./node_modules/.bin/tslint",
    "ts.tslint.configPath": "./tslint.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neo9/n9-node-conf"
  },
  "author": "Sebastien Chopin <sebastien.chopin@neo9.fr>",
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "debug": "^3.0.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/debug": "^0.0.30",
    "@types/lodash": "^4.14.74",
    "@types/node": "^8.0.25",
    "ava": "^0.22.0",
    "@neo9/n9-node-sonar-generate": "^1.1.0",
    "nyc": "^11.1.0",
    "rimraf": "^2.6.1",
    "std-mocks": "^1.0.1",
    "tslint": "^5.7.0",
    "typescript": "^2.4.2"
  }
}
